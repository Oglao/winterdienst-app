REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_SOCKET_URL=http://localhost:5000
REACT_APP_APP_NAME=Winterdienst Manager
REACT_APP_VERSION=1.0.0

# API Keys für externe Services
# OpenWeatherMap API (für Wetterdaten)
REACT_APP_OPENWEATHER_API_KEY=your_openweather_api_key_here

# Google Maps API (für Routenoptimierung)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# MapBox API (Alternative für Routenoptimierung)
REACT_APP_MAPBOX_API_KEY=your_mapbox_api_key_here

# Azure Maps API (für Azure Maps Features)
REACT_APP_AZURE_MAPS_KEY=your_azure_maps_key_here

# Photo Upload Endpoint
REACT_APP_UPLOAD_ENDPOINT=/api/photos/upload